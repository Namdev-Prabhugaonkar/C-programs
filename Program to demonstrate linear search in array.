#include<stdio.h>
#include<iostream>
#include<string.h>
#include<time.h>
#include<vector>

using namespace std;

int main() {
    int n, temp, swapped;
    int count = 10;

    int arr[count];
    for (int i = 0; i < count; i++) {
        arr[i] = i * 2 + 9;
    }

 
    for (int i = 0; i < count; i++) {
        cout << arr[i]<< " ";
    }

    cout << endl;

    cout << "Enter the element to be searched:";
    cin>>n;
    
  for (int i = 0; i < count; i++) {
      if(arr[i]==n){
          cout<<i;
          break;
      }
    }


}
