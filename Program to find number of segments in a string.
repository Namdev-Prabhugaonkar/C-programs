class Solution {
public:
    int countSegments(string s)
    {
        int length= s.length();
        s=s+' ';
        int count=0;
        if(length==0)return 0;
        else
        for(int i=0;i<length;i++)
         if(s[i]!=' '&&s[i+1]==' ')        
             count++;
       return count;
       
    }
};
