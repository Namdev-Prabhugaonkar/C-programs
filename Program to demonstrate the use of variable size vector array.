
#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
   int n;
   int q;
   int sa;
   int sb;
   int row;
   int column;
   //length of queues and number of queries
   cin>>n;
   cin>>q;


 vector <vector <int>> a(n);
for(int i=0;i<n;i++){
//size of first queue
   cin>>sa;
   a[i].resize(sa);
   for(int j=0;j<sa;j++){
    //filling of each queue
       cin>>a[i][j];
   }
}




 for(int i=0;i<q;i++){
      cin>>row;
 if(row==1){
     cin>>column;
     cout<<a[row][column]<<endl;
 }
 else{
     cin>>column;
     cout<<a[row][column]<<endl;
 }
 }
// for(int x=0;x<q;x++){
//     cin>>row;
//     cin>>column;
//     cout<<a[row][column]<<endl;
// }
  


    return 0;
}

