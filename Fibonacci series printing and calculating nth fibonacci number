#include <stdio.h>
#include <stdbool.h>
#include <math.h>


int fib(int n){
    if(n<=0) return 0;
    else if(n==1) return 1;
    return fib(n-1)+fib(n-2);
    
}
void allFiB(int n){
    for(int i=0;i<n;i++)
    {
        printf("%i ,",fib(i));
    }
}

int main() {
	//code
	int n=7;
	//printf("%i",fib(n));
	allFiB(n);
	return 0;
}
