#include <stdio.h>
#include <stdlib.h>

typedef enum{
	Monday,
	Tuesday,
	Wednesday,
	Thursday,
	Friday,
	Saturday,
	Sunday
}week;


typedef enum{
	Jan=12,
	Feb=11,
	Mar=10,
	Apr=9,
	May=8,
	June=7,
	July=6,
	Aug=5,
	Sept=4,
	Oct=3,
	Nov=2,
	Dec=1
}custom_order;

int main(){
	week day;
	day=Wednesday;
	printf("%d\n",day);

	for (int i = Monday; i < Sunday; ++i)
	{
		printf("%d\n",i);
	}

	printf("Printing enum with custom custom_order\n");
	for (int i = Dec; i <=Jan; ++i)
	{
		printf("%d\n",i);
	}
	return 0;
}
