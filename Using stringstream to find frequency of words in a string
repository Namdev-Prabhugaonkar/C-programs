#include <stdio.h>
#include <iostream>
#include <unordered_map> 
#include <bits/stdc++.h> 
using namespace std;

int main()
{
    unordered_map <string,int> map;
    
    string str="one two three one two three";
    stringstream s(str);
    string temp;
    while(s>>temp){
        map[temp]++;
    }
    for(auto i: map){
        cout<<i.first<<" "<<i.second<<endl;
    }
    

    return 0;
}
