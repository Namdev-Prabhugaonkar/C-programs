

class Solution {
public:
    vector<int> sortArrayByParity(vector<int>& A) {
         for(int i=0;i<A.size();i++){
              for(int j=0;j<A.size();j++){
                 if(A[i]%2==0){
                     swap(A[i],A[j]);
                 }
             }            
         }
        return A;
    }
};
